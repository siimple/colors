//Check if the siimple object exists
if(typeof window.siimple === 'undefined'){ window.siimple = {}; }

//Initialize the colors object
window.siimple.colors = {};

{{#each list as |color|}}
//{{capitalize color.name}} color palette
window.siimple.colors.{{ color.name }} = [ {{#each color.values}}'{{this}}'{{#unless @last}}, {{/unless}}{{/each}} ];

{{/each}}