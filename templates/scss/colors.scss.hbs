//Import dependencies
@import "./_palette.scss";
@import "./_functions.scss";

{{#each colors}}
//{{this.name}} color
$siimple-{{this.name}}-extra-dark: siimple-color("{{this.name}}", "extra-dark");
$siimple-{{this.name}}-dark: siimple-color("{{this.name}}", "dark");
$siimple-{{this.name}}: siimple-color("{{this.name}}", "base");
$siimple-{{this.name}}-light: siimple-color("{{this.name}}", "light");
$siimple-{{this.name}}-extra-light: siimple-color("{{this.name}}", "extra-light");

{{/each}}

//White color 
$siimple-white: #ffffff;

